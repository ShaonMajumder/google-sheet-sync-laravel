{
    "openapi": "3.0.0",
    "info": {
        "title": "Google Sheet Sync Laravel API",
        "description": "A Laravel-based API to synchronize data with Google Sheets. This API allows authentication using OAuth 2.0 and supports reading, writing, appending, and managing Google Spreadsheet data.",
        "contact": {
            "email": "smazoomder@gmail.com"
        },
        "version": "1.2.0"
    },
    "paths": {
        "/google-sheets/api/v0/create-spreadsheet": {
            "post": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Create a new Google Spreadsheet",
                "operationId": "0c8820a1cefa63ee26ab5d5bc654af23",
                "requestBody": {
                    "description": "Spreadsheet creation payload",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "example": "Swagger test"
                                    },
                                    "data": {
                                        "type": "array",
                                        "items": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        "example": [
                                            [
                                                "Name",
                                                "Age",
                                                "City"
                                            ],
                                            [
                                                "Alice",
                                                "30",
                                                "New York"
                                            ],
                                            [
                                                "Bob",
                                                "25",
                                                "Los Angeles"
                                            ]
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Spreadsheet created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "spreadsheetId": {
                                            "type": "string",
                                            "example": "1x2y3z..."
                                        },
                                        "spreadsheetUrl": {
                                            "type": "string",
                                            "example": "https://docs.google.com/spreadsheets/d/1x2y3z..."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Error occurred while creating spreadsheet",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Failed to create spreadsheet"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/google-sheets/api/v0/create-sheet/{spreadsheetId}/{sheetName}": {
            "post": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Create a new sheet in the spreadsheet",
                "operationId": "3890af0bf79be02b9bea7320f0a9dce7",
                "parameters": [
                    {
                        "name": "spreadsheetId",
                        "in": "path",
                        "description": "ID of the spreadsheet where the new sheet will be created",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sheetName",
                        "in": "path",
                        "description": "Name of the new sheet to be created",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Optional sheet title and data to populate",
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "example": "Shaon New Spreadsheet"
                                    },
                                    "data": {
                                        "type": "array",
                                        "items": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        "example": [
                                            [
                                                "Name",
                                                "Age",
                                                "City"
                                            ],
                                            [
                                                "Alice",
                                                "30",
                                                "New York"
                                            ],
                                            [
                                                "Bob",
                                                "25",
                                                "Los Angeles"
                                            ]
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Sheet created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "sheetId": {
                                            "type": "integer",
                                            "example": 1289357324
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Sheet 'swagger-sheet-1' created successfully."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Error occurred while creating the sheet",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Failed to create sheet"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/google-sheets/api/v0/delete-spreadsheet/{spreadsheetId}": {
            "delete": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Delete a specific spreadsheet",
                "description": "Deletes a Google Spreadsheet by its ID.",
                "operationId": "deleteSpreadsheet",
                "parameters": [
                    {
                        "name": "spreadsheetId",
                        "in": "path",
                        "description": "The ID of the spreadsheet to delete.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Spreadsheet successfully deleted",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Spreadsheet successfully deleted"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Spreadsheet not found or could not be deleted",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Spreadsheet not found or could not be deleted"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Failed to delete spreadsheet"
                                        },
                                        "details": {
                                            "type": "string",
                                            "example": "Error details"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/google-sheets/api/v0/delete-sheet/{spreadsheetId}/{sheetName}": {
            "delete": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Delete a specific sheet in a Google Spreadsheet",
                "description": "Deletes a sheet in the specified Google Spreadsheet by its name.",
                "operationId": "deleteSheet",
                "parameters": [
                    {
                        "name": "spreadsheetId",
                        "in": "path",
                        "description": "The ID of the spreadsheet from which the sheet will be deleted.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sheetName",
                        "in": "path",
                        "description": "The name of the sheet to delete.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sheet successfully deleted",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sheet successfully deleted"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Sheet not found or could not be deleted",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Sheet not found or could not be deleted"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Failed to delete sheet"
                                        },
                                        "details": {
                                            "type": "string",
                                            "example": "Error details"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/google-sheets/api/v0/read-sheet/{spreadsheetId}/{sheetName}": {
            "get": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Read data from a specific sheet in a Google Spreadsheet",
                "description": "Fetches the data from the specified sheet in a Google Spreadsheet.",
                "operationId": "readSheet",
                "parameters": [
                    {
                        "name": "spreadsheetId",
                        "in": "path",
                        "description": "The ID of the spreadsheet to read data from.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sheetName",
                        "in": "path",
                        "description": "The name of the sheet to read data from.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Data fetched successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Data fetched successfully from 'Sheet1'."
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "array",
                                                "items": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "An error occurred: Error message"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/google-sheets/api/v0/insert-data/{spreadsheetId}/{sheetName}": {
            "post": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Insert data into a specific sheet in a Google Spreadsheet",
                "description": "Inserts data into the specified sheet of a Google Spreadsheet.",
                "operationId": "insertData",
                "parameters": [
                    {
                        "name": "spreadsheetId",
                        "in": "path",
                        "description": "The ID of the spreadsheet to insert data into.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sheetName",
                        "in": "path",
                        "description": "The name of the sheet to insert data into.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Insert data payload",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "data": {
                                        "type": "array",
                                        "items": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        "example": [
                                            [
                                                "ProductID",
                                                "Product Name",
                                                "Category",
                                                "Price",
                                                "Stock"
                                            ],
                                            [
                                                "P001",
                                                "Lapto2p",
                                                "Electronics",
                                                "1200",
                                                "50"
                                            ],
                                            [
                                                "P002",
                                                "Smartphone",
                                                "Electronics",
                                                "800",
                                                "100"
                                            ],
                                            [
                                                "P003",
                                                "Desk Chair",
                                                "Furniture",
                                                "150",
                                                "30"
                                            ]
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Data inserted successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Data inserted successfully into sheet 'Sheet1'."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request – Invalid input format",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Invalid data format"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not Found – Spreadsheet or Sheet does not exist",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Sheet not found"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized – Missing or invalid API key",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Unauthorized"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "An error occurred: Error message"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "api_key": []
                    }
                ]
            }
        },
        "/google-sheets/api/v0/append-data/{spreadsheetId}/{sheetName}": {
            "post": {
                "tags": [
                    "GoogleSheet"
                ],
                "summary": "Append data to a specific sheet in a Google Spreadsheet",
                "description": "Appends rows of data to the specified sheet in a Google Spreadsheet.",
                "operationId": "appendData",
                "parameters": [
                    {
                        "name": "spreadsheetId",
                        "in": "path",
                        "description": "The ID of the spreadsheet to append data to.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sheetName",
                        "in": "path",
                        "description": "The name of the sheet to append data to.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Insert data payload",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "data": {
                                        "type": "array",
                                        "items": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        "example": [
                                            [
                                                "ProductID",
                                                "Product Name",
                                                "Category",
                                                "Price",
                                                "Stock"
                                            ],
                                            [
                                                "P001",
                                                "Lapto2p",
                                                "Electronics",
                                                "1200",
                                                "50"
                                            ],
                                            [
                                                "P002",
                                                "Smartphone",
                                                "Electronics",
                                                "800",
                                                "100"
                                            ],
                                            [
                                                "P003",
                                                "Desk Chair",
                                                "Furniture",
                                                "150",
                                                "30"
                                            ]
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Row(s) appended successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Row appended successfully to sheet 'Sheet1'."
